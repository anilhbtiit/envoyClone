expression {
  call_expr {
    function: "_==_"
    args {
      select_expr {
        operand {
          select_expr {
            operand {
              ident_expr {
                name: "request"
              }
            }
            field: "query"
          }
        }
        field: "a"
      }
      const_expr {
        string_value: "apple"
      }
    }
  }
}

request_headers {
  headers{key: ":method" value : "GET"}
  headers{key: ":path" value : "/query?a=apple"}
  headers{key: ":scheme" value : "http"}
  headers{key: ":authority" value : "foo.com"}
  headers {}
  headers {}
  headers {}
  headers {}
}
response_headers {
  headers {
    key: ":status"
    value : "200"
  }
}
trailers {
  headers {}
}
