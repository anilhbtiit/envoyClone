config {
  virtual_hosts {
    name: "local_service"
    domains: "*"
    routes {
      match {
        prefix: "/foo"
      }
      route {
        cluster_header: ":authority"
      }
    }
    routes {
      match {
        prefix: "/bar"
      }
      route {
        cluster_header: "some_header"
        timeout {
        }
      }
    }
  }
}
headers {
  headers {
    key: ":authority"
    value: "some_cluster"
  }
  headers {
    key: ":path"
    value: "/foo"
  }
  headers {
    key: ":method"
    value: "GET"
  }
}
