configuration {
    merge_slashes: true
    http2_options { allow_extended_connect: true }
}

request {
    scheme { value: "https" }
    method { value: "CONNECT" }
    authority { value: "foo.bar.com" }
    protocol { value: "websocket" }
    path { value: "/some/path/..////websocket/./ttt?key=value" }
    headers {
        key: "foo"
        value: "bar"
    }
    headers {
        key: "baz"
        value: "fuz"
    }
}

response {
    status { value: "101" }
    headers {
        key: "upgrade"
        value: "websocket"
    }
    headers {
        key: "connection"
        value: "upgrade"
    }
    headers {
        key: "cow"
        value: "moo"
    }
    headers {
        key: "cat"
        value: "meow"
    }
}
