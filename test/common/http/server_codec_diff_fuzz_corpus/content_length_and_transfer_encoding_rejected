configuration {
    http1_options { allow_chunked_length: false }
}

request {
    scheme { value: "https" }
    method { value: "GET" }
    path { value: "/" }
    authority { value: "foo.bar.com" }
    headers {
        key: "content-length"
        value: "10"
    }
    headers {
        key: "transfer-encoding"
        value: "chunked"
    }
    headers {
        key: "baz"
        value: "fuz"
    }
}
