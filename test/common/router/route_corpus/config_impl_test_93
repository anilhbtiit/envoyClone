config {
  virtual_hosts {
    name: "local_service"
    domains: "*"
    routes {
      match {
        prefix: "/"
        headers {
          name: "content-type"
          exact_match: "application/grpc"
        }
      }
      route {
        cluster: "local_service_grpc"
      }
    }
    routes {
      match {
        prefix: "/"
      }
      route {
        cluster: "local_service"
      }
    }
  }
}
headers {
  headers {
    key: ":authority"
    value: "www.lyft.com"
  }
  headers {
    key: ":path"
    value: "/"
  }
  headers {
    key: ":method"
    value: "GET"
  }
  headers {
    key: "content-type"
    value: "foo"
  }
}
