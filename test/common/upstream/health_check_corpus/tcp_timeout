health_check_config {
    timeout {
        seconds: 1
    }
    interval {
        seconds: 1
    }
    unhealthy_threshold {
        value: 1
    }
    healthy_threshold: {
        value: 2
    }
    tcp_health_check {
        send {
            text: "01"
        }
        receive [{
            text: "02"
        }]
    }
}
actions {
    raise_event: CONNECTED
}
actions {
    respond {
        tcp_respond {
            data: "1"
        }
    }
}

actions {
    raise_event: REMOTE_CLOSE
}
