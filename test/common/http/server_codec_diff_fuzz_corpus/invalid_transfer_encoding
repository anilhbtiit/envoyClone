configuration {
    normalize_path: true
}

request {
    scheme { value: "https" }
    method { value: "GET" }
    path { value: "/" }
    authority { value: "foo.bar.com" }
    headers {
        key: "transfer-encoding"
        value: "identity"
    }
    headers {
        key: "transfer-encoding"
        value: "chunked, gzip"
    }
    headers {
        key: "baz"
        value: "fuz"
    }
}
