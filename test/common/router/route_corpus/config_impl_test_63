config {
  name: "foo"
  virtual_hosts {
    name: "www2"
    domains: "*"
    routes {
      match {
        prefix: "/endpoint"
      }
      route {
        cluster: "www2"
        request_headers_to_add {
          header {
            key: "x-global-header"
            value: "route-action-endpoint"
          }
          append {
          }
        }
        request_headers_to_add {
          header {
            key: "x-vhost-header"
            value: "route-action-endpoint"
          }
          append {
          }
        }
        request_headers_to_add {
          header {
            key: "x-route-header"
            value: "route-action-endpoint"
          }
        }
        request_headers_to_add {
          header {
            key: "x-route-action-header"
            value: "route-action-endpoint"
          }
          append {
          }
        }
      }
      request_headers_to_add {
        header {
          key: "x-global-header"
          value: "route-endpoint"
        }
        append {
        }
      }
      request_headers_to_add {
        header {
          key: "x-vhost-header"
          value: "route-endpoint"
        }
        append {
        }
      }
      request_headers_to_add {
        header {
          key: "x-route-header"
          value: "route-endpoint"
        }
        append {
        }
      }
    }
    routes {
      match {
        prefix: "/"
      }
      route {
        cluster: "www2"
      }
    }
    request_headers_to_add {
      header {
        key: "x-global-header"
        value: "vhost-www2"
      }
      append {
      }
    }
    request_headers_to_add {
      header {
        key: "x-vhost-header"
        value: "vhost-www2"
      }
      append {
      }
    }
  }
  request_headers_to_add {
    header {
      key: "x-global-header"
      value: "global"
    }
    append {
    }
  }
}
headers {
  headers {
    key: ":authority"
    value: "www.lyft.com"
  }
  headers {
    key: ":path"
    value: "/endpoint"
  }
  headers {
    key: ":method"
