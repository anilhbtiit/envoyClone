configuration {
    normalize_path: true
}

request {
    scheme { value: "https" }
    method { value: "GET" }
    path { value: "/websocket/endpoint" }
    authority { value: "foo.bar.com" }
    headers {
        key: "connection"
        value: "upgrade, keep-alive"
    }
    headers {
        key: "upgrade"
        value: "websocket"
    }
    headers {
        key: "baz"
        value: "fuz"
    }
}

response {
    status { value: "101" }
    headers {
        key: "upgrade"
        value: "websocket"
    }
    headers {
        key: "connection"
        value: "upgrade"
    }
    headers {
        key: "cow"
        value: "moo"
    }
    headers {
        key: "cat"
        value: "meow"
    }
}
