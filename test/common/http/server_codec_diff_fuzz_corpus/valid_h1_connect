configuration {
    normalize_path: true
}

request {
    scheme { value: "https" }
    method { value: "CONNECT" }
    path { value: "foo.bar.com:80" }
    headers {
        key: "foo"
        value: "bar"
    }
    headers {
        key: "proxy-authorization"
        value: "basic aGVsbG86d29ybGQ="
    }
}

response {
    status { value: "200" }
    headers {
        key: "something"
        value: "other"
    }
}
