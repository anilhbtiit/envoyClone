config {
  virtual_hosts {
    name: "www2"
    domains: "www.lyft.com"
    routes {
      match {
        prefix: "/"
      }
      route {
        cluster: "not_found"
        cluster_not_found_response_code: NOT_FOUND
      }
    }
  }
}
headers {
  headers {
    key: ":authority"
    value: "www.lyft.com"
  }
  headers {
    key: ":path"
    value: "/"
  }
  headers {
    key: ":method"
    value: "GET"
  }
}
