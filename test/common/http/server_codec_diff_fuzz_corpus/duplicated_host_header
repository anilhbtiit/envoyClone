configuration {
    normalize_path: true
}

request {
    scheme { value: "https" }
    method { value: "GET" }
    path { value: "/" }
    authority { value: "foo.bar.com" }
    headers {
        key: "foo"
        value: "bar"
    }
    headers {
        key: "host"
        value: "fuz.baz.net"
    }
}

response {
    status { value: "200" }
    headers {
        key: "1"
        value: "2"
    }
}
