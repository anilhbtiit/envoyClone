config {
  virtual_hosts {
    name: "default"
    domains: "*"
    routes {
      match {
        prefix: "/api"
      }
      route {
        cluster: "ats"
        cors {
          allow_origin: "test-origin"
          allow_methods: "test-methods"
          allow_headers: "test-headers"
          expose_headers: "test-expose-headers"
          max_age: "test-max-age"
          allow_credentials {
            value: true
          }
        }
      }
    }
  }
}
headers {
  headers {
    key: ":authority"
    value: "api.lyft.com"
  }
  headers {
    key: ":path"
    value: "/api"
  }
  headers {
    key: ":method"
    value: "GET"
  }
}
