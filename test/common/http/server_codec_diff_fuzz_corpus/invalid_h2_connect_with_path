configuration {
    normalize_path: true
}

request {
    method { value: "CONNECT" }
    authority { value: "foo.bar.com:80" }
    path { value: "/something/bad?should=not&be=here" }
    headers {
        key: "foo"
        value: "bar"
    }
    headers {
        key: "baz"
        value: "fuz"
    }
}
