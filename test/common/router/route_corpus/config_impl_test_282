config {
  name: "foo"
  virtual_hosts {
    name: "bar"
    domains: "*"
    routes {
      match {
        prefix: "/"
      }
      route {
        weighted_clusters {
          clusters {
            name: "baz"
            weight {
              value: 100
            }
            per_filter_config {
              key: "test.filter"
              value {
                fields {
                  key: "seconds"
                  value {
                    number_value: 789
                  }
                }
              }
            }
          }
        }
      }
    }
    per_filter_config {
      key: "test.filter"
      value {
        fields {
          key: "seconds"
          value {
            number_value: 1011
          }
        }
      }
    }
  }
}
