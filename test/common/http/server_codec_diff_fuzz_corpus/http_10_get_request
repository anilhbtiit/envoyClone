configuration {
    http1_options { 
        accept_http_10: true
        request_version: HTTP10 
    }
}

request {
    method { value: "GET" }
    path { value: "/some/resource?get=it&now" }
    headers {
        key: "foo"
        value: "bar"
    }
    headers {
        key: "baz"
        value: "fuz"
    }
}

response {
    status { value: "200" }
    headers {
        key: "dog_gy"
        value: "bark!bark"
    }
}
