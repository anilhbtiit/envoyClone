config {
  name: "foo"
  virtual_hosts {
    name: "direct"
    domains: "example.com"
    routes {
      match {
        prefix: "/"
      }
      direct_response {
        status: 200
        body {
          inline_string: "content"
        }
      }
    }
  }
}
headers {
  headers {
    key: ":authority"
    value: "example.com"
  }
  headers {
    key: ":path"
    value: "/"
  }
  headers {
    key: ":method"
    value: "GET"
  }
}
