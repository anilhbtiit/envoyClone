configuration {
    normalize_path: true
}

request {
    scheme { value: "https" }
    method { value: "GET" }
    path { value: "/" }
    authority { value: "foo.bar.com" }
    headers {
        key: "content-length"
        value: "10"
    }
    headers {
        key: "transfer-encoding"
        value: "chunked"
    }
    headers {
        key: "baz"
        value: "fuz"
    }
}

response {
    status { value: "200" }
    headers {
        key: "1"
        value: "2"
    }
}
