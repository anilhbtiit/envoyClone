config {
  name: "foo"
  virtual_hosts {
    name: "redirect"
    domains: "redirect.lyft.com"
    routes {
      match {
        prefix: "/"
      }
      redirect {
        host_redirect: "new.lyft.com"
        response_code: TEMPORARY_REDIRECT
      }
    }
  }
}
headers {
  headers {
    key: ":authority"
    value: "www.foo.com"
  }
  headers {
    key: ":path"
    value: "/foo"
  }
  headers {
    key: "x-forwarded-proto"
    value: "https"
  }
  headers {
    key: "x-envoy-internal"
    value: "true"
  }
}
